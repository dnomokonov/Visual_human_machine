<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:Homework_11_CustomTable.ViewModels"
			 xmlns:controls="using:Homework_11_CustomTable.Controls"
			 xmlns:models="using:Homework_11_CustomTable.Models"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:MainWindowViewModel"
             x:Class="Homework_11_CustomTable.Views.TableMainWindow">
	<Design.DataContext>
		<vm:MainWindowViewModel/>
	</Design.DataContext>
	<UserControl.Resources />

	<StackPanel>
		<controls:TableControl TargetObject="{Binding UserData}"/>
	</StackPanel>
	
</UserControl>
